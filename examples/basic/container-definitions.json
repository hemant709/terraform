[
  {
    "name": "nginx",
    "image": "nginx:stable",
    "essential": true,
    "portMappings": [
      { "containerPort": 80, "hostPort": 80, "protocol": "tcp" }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/example-ecs",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "ecs"
      }
    }
  }
]
